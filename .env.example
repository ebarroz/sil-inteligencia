# Configurações do Sistema SIL - Centro de Inteligência
# Copie este arquivo para .env e configure suas credenciais

# Banco de Dados PostgreSQL
DB_NAME=nome_do_banco
DB_USER=usuario_do_banco
DB_PASSWORD=senha_forte_aqui

# Cache Redis
REDIS_PASSWORD=senha_redis_aqui

# Interface pgAdmin
PGADMIN_EMAIL=seu_email@empresa.com
PGADMIN_PASSWORD=senha_pgadmin_aqui

# Configurações da Aplicação
NODE_ENV=production
TZ=America/Sao_Paulo
